{
  "name": "@markpage/svelte",
  "version": "0.1.1",
  "description": "Svelte integration for Markpage with component support",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src/lib"
  ],
  "scripts": {
    "build": "npx svelte-package",
    "dev": "npx svelte-package --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "dev:local": "npm run link:local && npm run dev",
    "dev:published": "npm run unlink:local && npm run dev",
    "link:local": "npm link markpage",
    "unlink:local": "npm unlink markpage && npm install markpage@^0.3.6"
  },
  "peerDependencies": {
    "svelte": "^5.38.6"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.37.0",
    "@sveltejs/package": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@types/node": "^20.0.0",
    "svelte": "^5.38.6",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "keywords": [
    "markpage",
    "svelte",
    "markdown",
    "components",
    "documentation"
  ],
  "author": "Markpage Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mitkury/markpage"
  },
  "dependencies": {
    "markpage": "^0.4.1"
  }
}
