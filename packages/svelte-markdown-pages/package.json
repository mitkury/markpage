{
  "name": "svelte-markdown-pages",
  "version": "0.1.0",
  "description": "Build and render markdown-based content with distributed navigation for Svelte projects",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./builder": {
      "types": "./dist/builder/index.d.ts",
      "import": "./dist/builder/index.js",
      "require": "./dist/builder/index.cjs"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js",
      "require": "./dist/renderer/index.cjs"
    },
    "./components": {
      "types": "./dist/renderer/components.d.ts",
      "import": "./dist/renderer/components.js",
      "require": "./dist/renderer/components.cjs"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist && cp ../../README.md .",
    "build": "tsup",
    "dev": "tsup --watch",
    "pretest": "npm run build",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "marked": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/node": "^20.0.0",
    "svelte": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "bin": {
    "svelte-markdown-pages": "./bin/svelte-markdown-pages.js"
  },
  "keywords": [
    "markdown",
    "documentation",
    "static-site",
    "navigation",
    "content-management",
    "svelte",
    "sveltekit",
    "docs",
    "documentation-generator"
  ],
  "author": "Dmitry Kury (d@dkury.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitkury/svelte-markdown-pages.git"
  },
  "homepage": "https://github.com/mitkury/svelte-markdown-pages#readme",
  "bugs": {
    "url": "https://github.com/mitkury/svelte-markdown-pages/issues"
  }
}
