{
  "name": "markpage",
  "version": "0.3.1",
  "description": "Build and manage markdown-based content with distributed navigation - framework agnostic content management system",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./builder": {
      "types": "./dist/builder/index.d.ts",
      "import": "./dist/builder/index.js",
      "require": "./dist/builder/index.cjs"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js",
      "require": "./dist/renderer/index.cjs"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist && cp ../../README.md .",
    "build": "tsup",
    "dev": "tsup --watch",
    "pretest": "npm run build",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "marked": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "bin": {
    "markpage": "bin/markpage.js"
  },
  "keywords": [
    "markdown",
    "documentation",
    "static-site",
    "navigation",
    "content-management",
    "framework-agnostic",
    "docs",
    "documentation-generator",
    "markdown-builder",
    "content-bundler"
  ],
  "author": "Dmitry Kury (d@dkury.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitkury/markpage.git"
  },
  "homepage": "https://github.com/mitkury/markpage#readme",
  "bugs": {
    "url": "https://github.com/mitkury/markpage/issues"
  }
}
